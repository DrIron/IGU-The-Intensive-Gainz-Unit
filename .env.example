# Supabase Configuration
# Copy this file to .env.local and fill in your values
# Get these from: https://supabase.com/dashboard/project/_/settings/api

# Your Supabase project reference ID
VITE_SUPABASE_PROJECT_ID="your-project-ref"

# Your Supabase anon/public key (safe to expose in frontend)
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-key"

# Your Supabase project URL
VITE_SUPABASE_URL="https://your-project-ref.supabase.co"

# Sentry Error Tracking (optional)
# Get your DSN from: https://sentry.io/settings/projects/YOUR_PROJECT/keys/
VITE_SENTRY_DSN="your-sentry-dsn"

# Set to "true" to enable Sentry in development mode
# VITE_SENTRY_DEBUG="true"
